{
    "name": "home/animatee-bundle",
    "type": "contao-bundle",
    "description": "Adds animation to Contao 4",
    "keywords":["contao","animatee","animation","aos"],
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "HolsteinMedia",
            "homepage": "http://www.holsteinmedia.com"
        }
    ],
    "require": {
        "php": "^5.6|^7.1",
        "contao/core-bundle": "^4.4.2",
        "home/pearls-bundle": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Home\\AnimateeBundle\\": ""
        },
        "classmap": [
            "Resources/contao/"
        ],
        "exclude-from-classmap": [
            "Resources/contao/config/",
            "Resources/contao/templates/"
        ]
    },
    "extra": {
        "contao-manager-plugin": "Home\\AnimateeBundle\\ContaoManager\\Plugin"
    }
}
